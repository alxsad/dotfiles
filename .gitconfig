[color]
    ui = true
[alias]
    lg = log --graph --color --pretty=format:'%C(red)%h%C(reset) -%C(yellow)%d%C(reset) %s %C(green)(%cr) %C(bold blue)<%an>%C(reset)' --
    co = checkout
    ci = commit
    st = status
    br = branch
    aa = add .
[push]
    default = current
[http]
    sslVerify = true
[core]
    editor = vim

